insert into test0.benchmark set state='replica is processing load';
start replica sql_thread;
use test;
select * from test0.benchmark into outfile 'benchmark.out';
select ts from test0.benchmark where state like 'primary started load' into @m_0;
select ts from test0.benchmark where state like 'primary ends load' into @m_1;
select ts from test0.benchmark where state like 'replica takes on load' into @s_0;
select ts from test0.benchmark where state like 'replica ends load' into @s_1;
select time_to_sec(@m_1) - time_to_sec(@m_0) as 'delta.out';
